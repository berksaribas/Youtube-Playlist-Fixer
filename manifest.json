{

  "manifest_version": 2,
  "name": "Youtube Playlist Fixer",
  "version": "1.3",

  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/watch?v=*&list=*", "https://youtube.com/watch?v=*&list=*", "https://youtube.com/watch?v=*&index=*&list=*", "https://www.youtube.com/watch?v=*&index=*&list=*"],
      "js": ["fixer.js"]
    }
  ]

}